    instance_create(0, 0, obj_item_stats);
}

// create player_stats object if there is not one already
if (!instance_exists(obj_player_stats)){
    instance_create(100, 100, obj_player_stats);
}

// create vars for fade out and in effect
darken = 0;
darken_step = 0;

// create vars for bottom gui
show_bottom_gui = false;


// randomize random numbers
randomize();
