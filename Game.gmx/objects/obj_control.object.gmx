with (obj_control) {
    count_vars++
}
if(count_vars &gt; 1) {
    with (self) instance_destroy(); 
    exit;
}

game_state = PLAY;

// Window
ww = min(1536,display_get_width()*0.90);
wh = ww/2;

window_set_size(ww, wh);
window_set_position(50, 50);
display_set_gui_size(512*3, 256*3);

